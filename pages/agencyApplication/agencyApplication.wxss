/* pages/agencyApplication/agencyApplication.wxss */
@import '../myOrder/myOrder.wxss';
.head{
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 50px;
  padding: 0 20px;
  background-color: #fff;
}
.head view{
  width: 40%;
  height: 100%;
  text-align: center;
  line-height: 50px;
}
.active{
  color: #2fcafa;
  border-bottom: 1px solid #2fcafa;
}
.order_list_item1_line3 {
  justify-content: space-between;
  align-items: center;
  border-top: 1px solid #eee;
  padding: 0 10px;
  font-size: 14px;
}
button{
  width: unset !important;
  padding: 0 10px;
  line-height: 30px;
  background-color: #fd0047;
  color: #fff;
}