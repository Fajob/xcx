/* pages/awardResult/awardResult.wxss */
page{
  background-color: #fff; 
  font-size: 16px;
}
.table {
  border: none;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}

.td {
  width: 25%;
  justify-content: center;
  text-align: center;
  font-size: 14px;
  overflow-wrap: break-word;
}
.td button{
  font-size: 14px;
  line-height: unset;
  background-color: #2fcafa;
  color: #fff;
  padding: 5px;
  width: 80px;
  border-radius: 25px;
}

.bg-w {
  border-bottom: 2px solid #eee;
}

.bg-g {
  border-bottom: 1px solid #eee;
}

.th {
  width: 25%;
  justify-content: center;
  color: #2fcafa;
  display: flex;
  height: 3rem;
  align-items: center;
}
